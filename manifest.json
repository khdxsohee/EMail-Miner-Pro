{
  "manifest_version": 3,
  "name": "EMail Miner Pro",
  "version": "6.12.10",
  "description": "Extract emails & websites smartly. Professional tool by Khalid Software House.",
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}